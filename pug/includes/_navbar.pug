header
  div.search-results.card

  div.navbar-fixed
    nav.primary
      div.nav-wrapper
        a.sidenav-trigger(href="#" data-target="nav-mobile")
          i.material-icons menu
        ul.left.hide-on-small-only
          li
            div.searchbar.has-sidebar
              span.prefix
                i.material-icons search
              input#searchbar.search-docs(type="search" required autofocus placeholder="Search..." autocomplete="off")
        ul.right
          li
            a#theme-switch(href='#')
              i.material-icons dark_mode
          li
            a(href="https://github.com/materializecss/materialize" target="_blank" class="ext-link" title="Check out our GitHub")
              img(src='images/github.svg' style="transform: scale(0.8);")
          li
            a(href="https://opencollective.com/materialize" target="_blank" class="ext-link" title="Support us via OpenCollective")
              img(src='images/opencollective.svg')
  
  if no_nav == false
    nav.top-nav
      .container
        .nav-wrapper
          .row
            .col.s12.m10.offset-m1
              h1.header #{page}

